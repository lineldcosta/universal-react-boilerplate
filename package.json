    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "express": "^4.15.0",
    "handlebars": "^4.0.6",
    "hbs": "^4.0.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "devDependencies": {
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "postcss-loader": "^1.3.3",
